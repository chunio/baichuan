{
  "name": "baichuan/library",
  "type": "library",
  "description": "a common library for [php]hyperf",
  "keywords": ["php", "hyperf", "common", "library"],
  "license": "MIT",
  "archive": {
    "exclude": [
      "/bin",
      "*.md",
      "/.*"
    ]
  },
  "require": {
    "php": ">=8.1",
    "hyperf/cache": "~3.1.0",
    "hyperf/command": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/db-connection": "~3.1.0",
    "hyperf/framework": "~3.1.0",
    "hyperf/guzzle": "~3.1.0",
    "hyperf/http-server": "~3.1.0",
    "hyperf/logger": "~3.1.0",
    "hyperf/memory": "~3.1.0",
    "hyperf/process": "~3.1.0",
    "hyperf/database": "~3.1.0",
    "hyperf/redis": "~3.1.0",
    "hyperf/constants": "~3.1.0",
    "hyperf/async-queue": "~3.1.0",
    "hyperf/amqp": "~3.1.0",
    "hyperf/model-cache": "~3.1.0",
    "hyperf/elasticsearch": "~3.1.0",
    "hyperf/tracer": "~3.1.0",
    "hyperf/validation": "^3.1",
    "hyperf/resource": "~3.1.0",
    "hyperf/gotask": "^3.0",
    "geoip2/geoip2": "^2.13",
    "hyperf/kafka": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/devtool": "~3.1.0",
    "hyperf/ide-helper": "~3.1.0",
    "hyperf/testing": "~3.1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "swoole/ide-helper": "^5.0",
    "hyperf/watcher": "^3.1"
  },
  "autoload": {
    "files": [
      "src/Utility/Common.php"
    ],
    "psr-4": {
      "Baichuan\\Library\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "hyperf": {
      "config": "Baichuan\\Library\\ConfigProvider"
    }
  }
}
